<?xml version="1.0" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="models.Cables" table="cables">
        <id name="id" type="int" column="id">
        </id>
        <property name="articul" type="string" column="articul"/>
        <property name="diametr" type="int" column="diametr"/>
        <property name="mass" type="int" column="mass"/>
        <property name="voltage" type="string" column="voltage"/>
    </class>
</hibernate-mapping>