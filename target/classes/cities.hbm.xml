<?xml version="1.0" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="models.Cities" table="Cities">
        <id name="id" type="int" column="id">
        </id>
        <property name="city" type="string" column="city"/>
        <property name="region_id" type="int" column="region_id"/>
        <property name="snowarea" type="int" column="snowarea"/>
        <property name="windarea" type="int" column="windarea"/>
    </class>
</hibernate-mapping>