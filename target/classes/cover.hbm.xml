<?xml version="1.0" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="models.Cover" table="cover">
        <id name="id" type="int" column="id">
        </id>
        <property name="mass" type="int" column="mass"/>
        <property name="length" type="int" column="length"/>
        <property name="type" type="int" column="type"/>
        <property name="width" type="int" column="width"/>
        <property name="thickness" type="double" column="thickness"/>
        <property name="articul_new" type="string" column="articul_new"/>
        <property name="title_new" type="string" column="title_new"/>
    </class>
</hibernate-mapping>