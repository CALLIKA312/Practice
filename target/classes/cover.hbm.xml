<?xml version="1.0" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="models.Cover" table="cover">
        <id name="id" type="int" column="id">
        </id>
        <property name="mass" type="int" column="mass"/>
        <property name="title_display" type="string" column="title_display"/>
        <property name="construction_id" type="int" column="construction_id"/>
    </class>
</hibernate-mapping>